{
  "name": "lecture.express",
  "version": "1.0.0",
  "description": "Let's learn express!",
  "main": "index.js",
  "repository": "git@github.com:codechrysalis/lecture.express.git",
  "author": "Felix Kirmse <felix@codechrysalis.io>",
  "license": "UNLICENSED",
  "private": true,
  "scripts": {
    "test": "mocha ./spec -R mocha-spec-json-output-reporter --reporter-options fileName=mocha-result.json,hierarchy=true",
    "lint": "eslint ../ --ext js -c ./.eslintrc -f json -o lint-result.json --fix --ignore-pattern node_modules"
  },
  "mia": {
    "reports": [
      {
        "script": "test",
        "type": "mocha",
        "file": "mocha-result.json"
      },
      {
        "script": "lint",
        "type": "eslint",
        "file": "lint-result.json"
      }
    ]
  },
  "dependencies": {
    "express": "^4.17.1"
  },
  "devDependencies": {
    "chai": "^4.2.0",
    "chai-http": "^4.3.0",
    "eslint": "^6.1.0",
    "eslint-config-prettier": "^6.0.0",
    "eslint-config-problems": "^3.0.1",
    "eslint-plugin-prettier": "^3.1.0",
    "mocha": "^6.2.0",
    "mocha-spec-json-output-reporter": "^1.1.7",
    "nodemon": "^1.19.1",
    "prettier": "^1.18.2",
    "pretty-quick": "^1.11.1"
  }
}
